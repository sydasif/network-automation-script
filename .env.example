# Network Automation Environment Variables
# Copy this file to .env and update with your actual values
# Keep .env file secure and do not commit to version control

# Network Device Credentials
NETWORK_USERNAME=admin
NETWORK_PASSWORD=cisco
NETWORK_ENABLE_PASSWORD=cisco

# Device IP Addresses
DEVICE_R1_IP=192.168.121.101
DEVICE_S1_IP=192.168.121.102

# Default timeout (in seconds)
NETWORK_TIMEOUT=30